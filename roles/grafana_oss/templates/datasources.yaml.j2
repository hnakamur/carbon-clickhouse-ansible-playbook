apiVersion: 1

# https://grafana.com/docs/grafana/latest/datasources/graphite/
datasources:
  - name: Carbonapi
    type: graphite
    access: proxy
    url: http://localhost:8081
    jsonData:
      graphiteVersion: '1.1'

  # https://grafana.com/grafana/plugins/grafana-clickhouse-datasource/?tab=overview
  - name: ClickHouse
    type: grafana-clickhouse-datasource
    jsonData:
      port: 9000
      host: localhost
      username: '{{ grafana_clickhouse_user }}'
      tlsSkipVerify: false
    secureJsonData:
      password: '{{ grafana_clickhouse_password }}'
